{
  "name": "react-jupyter-display-area",
  "version": "0.4.1",
  "description": "Jupyter Display Area as a React Component",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src/*",
    "prepublish": "npm run build",
    "prebuild": "mkdirp lib && npm run clean",
    "build": "npm run build:es5",
    "build:es5": "babel src --out-dir lib/ --source-maps",
    "build:watch": "babel src --watch --out-dir lib/ --source-maps",
    "test": "electron-mocha --renderer --compilers js:babel-core/register 'test/**/*.@(js|jsx)'",
    "test:watch": "npm run test -- --watch",
    "clean": "rimraf lib/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nteract/react-jupyter-display-area.git"
  },
  "keywords": [
    "jupyter",
    "electron",
    "notebook",
    "nteract"
  ],
  "author": "Kyle Kelley <rgbkrk@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/nteract/react-jupyter-display-area/issues"
  },
  "homepage": "https://github.com/nteract/react-jupyter-display-area#readme",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "peerDependencies": {
    "immutable": "^3.8.1",
    "react": "^0.14.0 || ^15.0.0-0",
    "react-dom": "^0.14.0 || ^15.0.0-0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.3",
    "babel-plugin-transform-class-properties": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.4",
    "electron-mocha": "^1.2.2",
    "electron-prebuilt": "^0.36.3",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^5.0.1",
    "estraverse-fb": "^1.3.1",
    "immutable": "^3.8.1",
    "jsx-chai": "^3.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.4",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "rimraf": "^2.5.0"
  },
  "dependencies": {
    "ansi-to-react": "0.0.4",
    "transformime-react": "^0.6.2"
  }
}
